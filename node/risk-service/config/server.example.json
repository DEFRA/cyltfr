{
  "server": {
    "host": "127.0.0.1",
    "port": 8050
  },
  "logging": {
    "ops": {
      "interval": 10000
    },
    "reporters": {
      "console": [
        {
          "module": "good-squeeze",
          "name": "Squeeze",
          "args": [
            {
              "log": "*",
              "request": "*",
              "response": "*",
              "error": "*"
            }
          ]
        },
        {
          "module": "good-console"
        },
        "stdout"
      ],
      "opsLogFile": [
        {
          "module": "good-squeeze",
          "name": "Squeeze",
          "args": [{
            "ops": "*"
          }]
        },
        {
          "module": "good-squeeze",
          "name": "SafeJson"
        },
        {
          "module": "good-file",
          "args": ["./logs/ops.log"]
        }
      ],
      "errorLogFile": [
        {
          "module": "good-squeeze",
          "name": "Squeeze",
          "args": [
            {
              "log": "error",
              "request": "error",
              "response": "error",
              "error": "*"
            }
          ]
        },
        {
          "module": "good-squeeze",
          "name": "SafeJson"
        },
        {
          "module": "good-file",
          "args": ["./logs/error.log"]
        }
      ]
    }
  },
  "database": {
    "connectionString": "postgres://u_ltfri:u_ltfri@localhost:5432/ltfridev"
  },
  "errbit": {
    "postErrors": false,
    "env": "development",
    "key": "replace_this",
    "host": "http://dummyuri",
    "proxy": ""
  }
}
