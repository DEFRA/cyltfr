{
  "server": {
    "host": "127.0.0.1",
    "port": 8050
  },
  "logging": {
    "reporters": [{
      "reporter": "good-console",
      "events": { "log": ["info","error"], "request": "*", "response": "*", "error": "*" }
    }, {
      "reporter": "good-file",
      "events": { "log": "info" },
      "config": { "path": "logs", "prefix": "server", "rotate": "daily" }
    }, {
      "reporter": "good-file",
      "events": { "ops": "*" },
      "config": { "path": "logs", "prefix": "ops", "rotate": "daily" }
    }, {
      "reporter": "good-file",
      "events": { "log": "error", "error": "*", "request": "error" },
      "config": { "path": "logs", "prefix": "error", "rotate": "daily" }
    }]
  },
  "database": {
    "connectionString": "postgres://u_ltfri:u_ltfri@localhost:5432/ltfridev"
  },
  "errbit": {
    "postErrors": false,
    "env": "development",
    "key": "replace_this",
    "host": "http://dummyuri",
    "proxy": ""
  }
}
