{
  "server": {
    "protocol": "http",
    "host": "127.0.0.1",
    "port": 3001
  },
  "service": {
    "protocol": "http",
    "host": "127.0.0.1",
    "port": 8050
  },
  "geoserver": {
    "protocol": "http",
    "host": "127.0.0.1",
    "port": 8080
  },
  "logging": {
    "ops": {
      "interval": 10000
    },
    "reporters": {
      "console": [
        {
          "module": "good-squeeze",
          "name": "Squeeze",
          "args": [
            {
              "log": "*",
              "request": "*",
              "response": "*",
              "error": "*"
            }
          ]
        },
        {
          "module": "good-console"
        },
        "stdout"
      ],
      "opsLogFile": [
        {
          "module": "good-squeeze",
          "name": "Squeeze",
          "args": [{ "ops": "*" }]
        },
        {
          "module": "good-squeeze",
          "name": "SafeJson"
        },
        {
          "module": "good-file",
          "args": ["./logs/ops.log"]
        }
      ],
      "errorLogFile": [
        {
          "module": "good-squeeze",
          "name": "Squeeze",
          "args": [{ "error": "*", "log": "error" }]
        },
        {
          "module": "good-squeeze",
          "name": "SafeJson"
        },
        {
          "module": "good-file",
          "args": ["./logs/error.log"]
        }
      ]
    }
  },
  "httpTimeoutMs": 10000,
  "mountPath": "",
  "analyticsAccount": "",
  "cacheViews": false,
  "mockAddressService": false,
  "floodWarningsUrl": "https://flood-warning-information.service.gov.uk/",
  "ordnanceSurvey": {
    "key": "",
    "urlUprn": "",
    "urlPostcode": "",
    "urlGetCapabilities": ""
  },
  "errbit": {
    "postErrors": false,
    "env": "development",
    "key": "replace_this",
    "host": "http://dummyuri",
    "proxy": ""
  }
}
