{
  "server": {
    "protocol": "http",
    "host": "127.0.0.1",
    "port": 3001,
    "load": {
      "sampleInterval": 0,
      "maxEventLoopDelay": 0
    }
  },
  "floodRiskService": {
    "protocol": "http",
    "host": "127.0.0.1",
    "port": 8050
  },
  "gazetteer" : {
    "protocol": "http",
    "host": "127.0.0.1",
    "port": 3002
  },
  "geoserver": {
    "protocol": "http",
    "host": "127.0.0.1",
    "port": 8080
  },
  "logging": {
  "reporters": [{
    "reporter": "good-console",
    "events": { "log": ["info","error"], "request": "*", "response": "*", "error": "*" }
  }, {
    "reporter": "good-file",
    "events": { "log": "info" },
    "config": { "path": "logs", "prefix": "server", "rotate": "daily" }
  }, {
    "reporter": "good-file",
    "events": { "ops": "*" },
    "config": { "path": "logs", "prefix": "ops", "rotate": "daily" }
  }, {
    "reporter": "good-file",
    "events": { "log": "error", "error": "*", "request": "error" },
    "config": { "path": "logs", "prefix": "error", "rotate": "daily" }
  }]
  },
  "pageRefreshTime": 900,
  "cacheExpiry": 900000,
  "analyticsAccount" : ""
}
