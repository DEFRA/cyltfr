{
  "server": {
    "protocol": "http",
    "host": "127.0.0.1",
    "port": 3001
  },
  "service": {
    "protocol": "http",
    "host": "127.0.0.1",
    "port": 8050
  },
  "geoserver": {
    "protocol": "http",
    "host": "127.0.0.1",
    "port": 8080
  },
  "logging": {
    "reporters": [{
      "reporter": "good-console",
      "events": { "log": ["info","error"], "request": "*", "response": "*", "error": "*" }
    }, {
      "reporter": "good-file",
      "events": { "log": "info" },
      "config": { "path": "logs", "prefix": "server", "rotate": "daily" }
    }, {
      "reporter": "good-file",
      "events": { "ops": "*" },
      "config": { "path": "logs", "prefix": "ops", "rotate": "daily" }
    }, {
      "reporter": "good-file",
      "events": { "log": "error", "error": "*", "request": "error" },
      "config": { "path": "logs", "prefix": "error", "rotate": "daily" }
    }]
  },
  "httpTimeoutMs": 10000,
  "analyticsAccount": "",
  "cacheViews": false,
  "floodWarningsUrl": "https://flood-warning-information.service.gov.uk/",
  "ordnanceSurvey": {
    "key": "",
    "urlUprn": "",
    "urlPostcode": ""
  }
}
